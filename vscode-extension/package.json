{
  "name": "sathi-lang",
  "displayName": "Sathi Language",
  "description": "Code like you speak, friendly like a friend.",
  "version": "0.1.0",
  "publisher": "NirajanG",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sathi",
        "aliases": [
          "Sathi",
          "sathi"
        ],
        "extensions": [
          ".sathi"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sathi",
        "scopeName": "source.sathi",
        "path": "./syntaxes/sathi.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "sathi",
        "path": "./snippets/sathi.json"
      }
    ]
  }
}